[user]
    email = brock@oregonstate.edu
    name = Stacy Brock
[push]
    default = simple
[core]
    excludesfile = ~/.gitignore
    autocrlf = input
[alias]
    lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ds = diff --staged
    upstatus = log HEAD..upstream/master --oneline
    ctags = !.git/hooks/ctags
    s = status -s
    root = "!f() { GITROOT=`pwd`; echo $GITROOT; }; f"
    pch = !.git/hooks/pre-commit
    wd = diff --word-diff=plain
    wds = diff --staged --word-diff=plain
[init]
    templatedir = ~/.git_template
	defaultBranch = main
[pull]
    rebase = true
[pager]
    branch = false
    tag = false
    stash = false
    config = false
